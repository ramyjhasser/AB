@article{ WOS:000382819000010,
Author = {Cole, David A. and Sinclair-McBride, Keneisha R. and Zelkowitz, Rachel
   and Bilsk, Sarah A. and Roeder, Kathryn and Spinelli, Tawny},
Title = {Peer Victimization and Harsh Parenting Predict Cognitive Diatheses for
   Depression in Children and Adolescents},
Journal = {JOURNAL OF CLINICAL CHILD AND ADOLESCENT PSYCHOLOGY},
Year = {2016},
Volume = {45},
