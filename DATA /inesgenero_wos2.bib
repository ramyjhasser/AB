@article{ WOS:000814345800001,
Author = {Weisz, Erika and Chen, Patricia and Ong, Desmond C. and Carlson, Ryan W.
   and Clark, Marissa D. and Zaki, Jamil},
Title = {A Brief Intervention to Motivate Empathy Among Middle School Students},
Journal = {JOURNAL OF EXPERIMENTAL PSYCHOLOGY-GENERAL},
Year = {2022},
Volume = {151},
Number = {12},
Pages = {3144-3153},
Month = {DEC},
Abstract = {Empathy tracks socioemotional adjustment during early adolescence, yet
   adolescents this age tend to show reductions in empathy compared with
   younger children. Here we took a novel approach to building empathy
   among early adolescents in four middle schools (n = 857). Rather than
   addressing the ability to empathize, we targeted the motivation to
   empathize. To do so, we leveraged strategies demonstrated to change
   motivation among early adolescents: social norms and mindsets. Compared
   with those in other conditions, students who received a norms-based
   intervention reported greater motivation to empathize with others, which
   was in turn associated with increased peer-reported prosocial behaviors,
   as well as lower levels of loneliness and aggression. The effects of
   this norms condition were strongest at schools with relatively high
   engagement with the intervention. Findings suggest a novel avenue for
   increasing empathy among early adolescents-focusing on peer-driven
   motivation-and underscore the importance of context in shaping
   intervention outcomes.},
