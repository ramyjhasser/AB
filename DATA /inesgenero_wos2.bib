@article{ WOS:000284821300016,
Author = {Kim, Eun Joo and Kim, Earnest S. and Covey, Ellen and Kim, Jeansok J.},
Title = {Social Transmission of Fear in Rats: The Role of 22-kHz Ultrasonic
   Distress Vocalization},
Journal = {PLOS ONE},
