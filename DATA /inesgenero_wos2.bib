@article{ WOS:001422212000016,
Author = {Caro, Pamela and Margarit, Daisy and Armijo, Lorena and Cardenas, Maria
   Elvira},
Title = {Subjective Precariousness and Recognition in the Work Trajectories of
   Seasonal Fruit Workers in Chile},
Journal = {UNIVERSUM-REVISTA DE HUMANIDADES Y CIENCIAS SOCIALES},
