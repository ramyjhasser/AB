@article{ WOS:000762911500001,
Author = {Li, Hua and Zhang, Qian},
Title = {Effects of Prosocial Video Games on Prosocial Thoughts and Prosocial
   Behaviors},
Journal = {SOCIAL SCIENCE COMPUTER REVIEW},
