@article{ WOS:000390571900001,
Author = {Yount, Kathryn Mary and Krause, Kathleen Helen and VanderEnde, Kristin
   E.},
Title = {Economic Coercion and Partner Violence Against Wives in Vietnam: A
   Unified Framework?},
Journal = {JOURNAL OF INTERPERSONAL VIOLENCE},
